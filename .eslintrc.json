{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "mongo": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "no-restricted-syntax": "off",
        "import/extensions": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off"
    }
}
